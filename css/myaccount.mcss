@charset 'UTF-8';
.m-myaccount{
    padding: 54px 40px 20px;
    //标题
    .title{
        font-size: 16px;
        font-weight: normal;
        color:#666;
        line-height: 16px;
        padding-left: 12px;
    }
    .list{
        margin-top: 24px;
        border-top: 1px solid #ebebeb;
        li{
            border-bottom: 1px dashed #dcdcdc;
            .name{
                font-size: 14px;
                float: left;
                height: 60px;
                line-height: 60px;
                width: 80px;
                text-align: right;
                color: #666;
            }
            .company{
                font-size: 14px;
                color: #333;
                float: left;
                line-height: 60px;
                padding-left: 76px;
            }
            .btn{
                width: 72px;
                height: 30px;
                line-height: 30px;
                border-radius: 3px;
                border: 1px solid #dcdcdc;
                color: #666;
                background: #f7f7f7;
                text-align: center;
                margin: 14px 18px 0 0;
                float: right;
                cursor: pointer;
            }
            .disabled-btn{
                float: right;
                color: #999;
                line-height: 60px;
                padding-right: 28px;
            }
        }
    }
}