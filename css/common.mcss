//复选框样式
.checkbox{
    background: url(../res/images/checkbox_fall.png) no-repeat;
    width: 12px;
    height: 12px;
    display: inline-block;
    cursor: pointer;
}
.checkbox-true{
    background: url(../res/images/checkbox_true.png) no-repeat;
}

.checkbox-big{
    background: url(../res/images/checkbox_b_off.png) no-repeat;
    width: 18px;
    height: 18px;
    display: inline-block;
    cursor: pointer;
}
.checkbox-big-true{
    background: url(../res/images/checkbox_b_on.png) no-repeat;
}
//单选框样式
.radio{
    background: url(../res/images/radiobutton_off.png) no-repeat;
    width: 14px;
    height: 14px;
    display: inline-block;
    cursor: pointer;
    vertical-align: middle;
}
.radio-true{
    background: url(../res/images/radiobutton_ON.png) no-repeat;
}